@SET_MAKE@
SHELL = @SHELL@
INSTALL = @INSTALL@
RM = @RM@ 

@NSDEJAVU_YES@SUBDIRS = bin2cpp djview nsdejavu
@NSDEJAVU_NO@SUBDIRS = bin2cpp djview

all install depend clean: FORCE
	for d in ${SUBDIRS} ;\
	 do ( cd $$d && ${MAKE} $@ ) || exit ; done

distclean: FORCE
	for d in ${SUBDIRS}; do ( cd $$d && ${MAKE} $@ ); done
	-${RM} Makefile 

FORCE:
.PHONY: FORCE










